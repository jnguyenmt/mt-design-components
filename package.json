{
  "name": "mt-design-components",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "main": "dist/mt-design-components.umd.js",
  "module": "dist/mt-design-components.es6.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Justin Nguyen <justin.nguyen@mineraltree.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --module es6 --p tsconfig.build.json && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --config jest.config.json --coverage",
    "test:watch": "jest --config jest.config.json --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "tslint -c tslint.json --project . --format verbose",
    "lint-fix": "tslint -c tslint.json --project . --fix --format verbose"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/redux-form": "^8.1.5",
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-config": "24.8.0",
    "jest-dom": "^3.1.2",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.8.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.14.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "publishConfig": {
    "registry": "http://localhost:8081/nexus/repository/npm-private/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
